.tips-tag {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 30rpx;
  padding: 30rpx 0;
  border-top: 1rpx solid rgba(0,0,0,.5);
  font-size: 24rpx;
  color: rgba(0,0,0,.7);
}
.tips-tag .left {
  display: flex;
  align-items: center;
}
.tips-tag .left .item-tag {
  position: relative;
  padding-left: 10rpx;
  margin-right: 20rpx;
  margin-left: 10rpx;
}
.tips-tag .left .item-tag::before {
  content: '';
  display: block;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #595959;
  position: absolute;
  left: -10rpx;
  top: 50%;
  transform: translateY(-50%);
}