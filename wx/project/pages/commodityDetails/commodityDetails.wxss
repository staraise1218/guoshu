/* Shopcart New */
.shopCart {
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
  z-index: 99;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15rpx 30rpx;
  box-sizing: border-box;
  border-top: 1rpx solid rgba(0,0,0,0.5);
}
.shopCart .addCart,
.shopCart .toPay {
  padding: 24rpx 20rpx;
  background: #FF4F76;
  text-align: center;
}
.shopCart .home {
  margin-right: 40rpx;
}
.shopCart .addCart {
  margin-left: 100rpx;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  border-right: 1rpx solid #fff;
}
.shopCart .toPay {
  border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
}
.shopCart .cart {
  position: relative;
}
.shopCart .cart .cartCount {
  position: absolute;
  right: -20rpx;
  top: -8rpx;
  width: 36rpx;
  height: 36rpx;
  background: #FF4F76;
  border-radius: 50%;
  line-height: 36rpx;
  text-align: center;
  color: #fff;
  font-size: 18rpx;
}


