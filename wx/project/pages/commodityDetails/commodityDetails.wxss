.alert_back {
  width: 100%;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,0.5);
  text-align: center;
  z-index: 999;
}
.alert_back .content {
  position: absolute;
  left: 50%;
  top: 200rpx;
  transform: translateX(-50%);
  background: #fff;
  padding: 20rpx;
  border-radius: 15rpx;
  font-size: 22rpx;
  color: #f40;
}
.alert_back .content image {
  width: 300rpx;
  height: 200rpx;
}
.alert_back .content .btn {
  border: 1px solid #f40;
  padding: 4rpx 10rpx;
  border-radius: 30rpx;
  color: #f40;
  margin-top: 20rpx;
}

