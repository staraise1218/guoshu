              <view class="shop-info">
                <view class="name">{{tuijianList[0].goods_name}}</view>
                <view class="name_">{{tuijianList[0].subtitle}}</view>
                <view class="huodong-price" style="margin-top: 38rpx;">
                  <view style="font-size: 20rpx;" wx:if="{{tuijianList[0].shop_price}}">￥</view>
                  <view style="color: #5A90E2; font-size: 26rpx; font-weight: 400" class="huodong-price_">
                    {{tuijianList[0].shop_price}}</view>
                </view>
              </view>
            </view>
            <view class="shop-item" data-goodsid="{{tuijianList[1].goods_id}}" data-id="{{tuijianList[1].goods_id}}" bindtap="toCon">
              <image style="width: 180rpx; height: 180rpx;"
                src="https://app.zhuoyumall.com:444{{tuijianList[1].original_img}}" />
              <view class="shop-info">
                <view class="name">{{tuijianList[1].goods_name}}</view>
                <view class="name_">{{tuijianList[1].subtitle}}</view>
                <view class="huodong-price" style="margin-top: 38rpx;">
                  <view style="font-size: 20rpx;" wx:if="{{tuijianList[1].shop_price}}">￥</view>
                  <view style="color: #5A90E2; font-size: 26rpx; font-weight: 400" class="huodong-price_">
                    {{tuijianList[1].shop_price}}</view>
                </view>
              </view>
            </view>
          </view>
          <!-- 推荐 END -->
        </view>
