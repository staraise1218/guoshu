              </view>
            </view>
            <view class="br" bindtap="toNext">下期预告 ></view>
          </view>
          <scroll-view scroll-x class="shoplist">
            <block wx:for="{{miaosha}}">
              <view class="shopitem"  data-goodsid="{{item.goods_id}}" data-id="{{item.goods_id}}" bindtap="toCon">
