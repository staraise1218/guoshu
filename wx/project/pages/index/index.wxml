                <view style="font-size: 24rpx; margin: 0 0 10rpx 0" class="name">{{item.goods_name}}</view>
                <view class="shop_ppice">
                  <view style="font-size: 24rpx;margin" wx:if="{{item.price}}">￥</view>
                  <view>{{item.price}}</view>
                </view>
              </view>
            </block>
          </scroll-view>
        </view>
      </view>
    </view>
