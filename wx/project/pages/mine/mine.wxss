.my-header {
    background-image: url('https://app.zhuoyumall.com:444/wx/img/2/我的顶部背景.jpg');
    height: 430rpx;
    background-repeat: no-repeat;
    background-size: cover;
    position: relative;
    font-size: 30rpx;
}
.my-header .my-header-pic {
    position: absolute;
    top: 72rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 102rpx;
    height: 102rpx;
    border-radius: 50%;
    overflow: hidden;
}
.my-header .my-header-pic .header-pic {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.my-header .my-name {
    position: absolute;
    top: 210rpx;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    justify-content: center;
    align-items: center;
}
.my-header .my-header-wrap {
    position: absolute;
    top: 310rpx;
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
.my-header .my-header-wrap .header-item {
    text-align: center;
    font-size: 26rpx;
    width: 200rpx;
}
.my-header .my-header-wrap .header-item view:last-child {
    margin-top: 10rpx;
}



.bg {
    background: #F5F5F5;
    padding: 20rpx;
}
.bg .my-item {
    background: #fff;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    padding: 20rpx;
    color: rgba(0,0,0,.6);
    margin-bottom: 20rpx;
}
.bg .my-item view {
    margin-left: 20rpx;
}
.more {
    position: relative;
}
.more::before {
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
    content: '';
    width: 30rpx;
    height: 30rpx;
    display: block;
    background-image: url('https://app.zhuoyumall.com:444/wx/img/2/右箭头.png');
    background-repeat: no-repeat;
    background-size: cover;
}
.blue {
    color: #5886ED !important;
}


.about-wrap {
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    padding: 30rpx 50rpx;
}
.about-wrap .about-item {
    text-align: center;
    font-size: 30rpx;
    color: rgba(0,0,0,.8);
    width: 200rpx;
}