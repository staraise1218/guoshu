.line-footer {
    position: relative;
    text-align: center;
    font-size: 28rpx;
    color: rgba(0,0,0,0.3)
}
.line-footer::after {
    position: absolute;
    left: 30rpx;
    top: 50%;
    transform: translateY(-50%);
    display: block;
    content: '';
    border: 1rpx solid rgba(0,0,0,0.1);
    width: 240rpx;
}
.line-footer::before {
    position: absolute;
    right: 30rpx;
    top: 50%;
    transform: translateY(-50%);
    display: block;
    content: '';
    border: 1rpx solid rgba(0,0,0,0.1);
    width: 240rpx;
}


