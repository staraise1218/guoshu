.footer {
    position: fixed;
    z-index: 500;
    bottom: 0;
    left: 0;
    width: 100%;
    border-top: 1rpx solid rgba(0,0,0,.05);
    background: #fff;
    height: 95rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 22rpx;
}
.footer image {
    width: 50rpx;
}
.footer .item-bar {
    display: flex;
    flex-direction: column;
    align-items: center;
    color: rgba(0,0,0,.5);
}
.footer .group-icon {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: -20rpx;
    width: 72rpx;
    height: 75rpx;
}
.footer .bar-icon {
    width: 55rpx;
    height: 55rpx;
}
.footer .shopcart-title {
    position: absolute;
    left: 50%;
    bottom: -6rpx;
    transform: translate(-50%, -50%);
}
.footer .active {
    color: #F7471C;
}
.footer .item-bar:nth-of-type(2) {
    margin-right: 30rpx;
}
.footer .item-bar:nth-of-type(4) {
    margin-left: 30rpx;
}

.shopcart-icon {
    position: relative;
}
.shopcart-num {
    position: absolute;
    right:-12rpx;
    top:-8rpx;
    font-size: 20rpx;
    background: #F7471C;
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    color: #fff;
    text-align: center;
    line-height: 30rpx;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

