<!-- 选择城市弹窗 -->
<view class="box" wx:if="{{cityShow}}">
  <view class="city-box">
    <view class="city-btn">
      <view class="cancel-btn" bindtap="cityCancel">取消</view>
      <view class="ok-btn" bindtap="cityOk">确定</view>
    </view>
    <picker-view indicator-style="height: 50px;" style="width: 100%; height: 300px;" value="{{value}}" bindchange="changeCity">
      <picker-view-column>
        <view wx:for="{{multiArray}}" style="line-height: 50px" wx:key>{{item.name}}</view>
      </picker-view-column>
      <picker-view-column>
        <view wx:for="{{multiArray[index0].sub}}" style="line-height: 50px" wx:key>
          {{item.name}}
        </view>
      </picker-view-column>
    </picker-view>
  </view>
</view>
<!-- 选择城市弹窗 END -->
<!-- 红包弹窗 -->
<view class="redBag-box" wx:if="{{youhuiAlert}}">
  <view class="content" bindtap="toCoup">
    <image src="https://app.zhuoyumall.com:444/wx/img/hongbao.png" />
    <view class="text">
      <view class="price">￥{{coup.money}}</view>
    </view>
    <image class="close" catchtap="alertClose" src="https://app.zhuoyumall.com:444/wx/img/wrong.png" />
  </view>
</view>
<!-- 红包弹窗 END -->
<!-- footer -->
<view class="footer" wx:if="{{false}}">
  <view class="item-bar" data-link='index' bindtap="LINK">
    <image class="bar-icon" src="https://app.zhuoyumall.com:444/wx/img/shouye.png" />
    <view class="title-bar active">首页</view>
  </view>
  <view class="item-bar" data-link='classification' bindtap="LINK">
    <view class="title-bar">分类</view>
  </view>
  <view class="item-bar" data-link='group' bindtap="LINK">
    <image class="group-icon" src="https://app.zhuoyumall.com:444/wx/img/tuangou.png" />
    <view class="title-bar shopcart-title">团购</view>
  </view>
  <view class="item-bar shopcart-icon" data-link='shoppingCart' bindtap="LINK">
    <image class="bar-icon" src="https://app.zhuoyumall.com:444/wx/img/gouwuche拷贝3@2x.png" />
    <view class="title-bar">购物车</view>
    <i wx:if="{{cart_num > 0}}" class="shopcart-num">{{cart_num}}</i>
  </view>
  <view class="item-bar" data-link='mine' bindtap="LINK">
    <image class="bar-icon" src="https://app.zhuoyumall.com:444/wx/img/wode@2x.png" />
    <view class="title-bar">我的</view>
  </view>
</view>
<!-- footer END -->