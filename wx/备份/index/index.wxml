<!-- 吸附顶部 -->
<view class="fix-wrap">
  <view class='topbar'>
    <!-- <view class="left-center logo-title title-2 margin-right-20">果蔬</view> -->
    <!-- <view  bindtap="cityshow"> -->
    <image bindtap="cityshow" class="location-icon" src="https://hbimg.huabanimg.com/698f1951de21b12c43072ae841182f165e05e9cd890-xBN8IX_fw658" />
    <view bindtap="cityshow" class="location" selectable="false" space="false" decode="false">
      {{address}}
    </view>
    <!-- </view> -->
    <view class="search" hover-class="none" hover-stop-propagation="false">
      <image class="search-icon" src="http://img.hb.aicdn.com/2428f6e809e53942c9be02543a31d0c59b3f7a286b0-2jNZQl_fw658" />
      <input class="serach-input" disabled='true' placeholder="火龙果" auto-focus bindtap="toSearch" />
    </view>
    <view bindtap="toNews" class="news" hover-class="none" hover-stop-propagation="false">
      <button class="news_btn" send-message-title="分享标题" send-message-img="分享的单个图片链接" show-message-card="true" send-message-path="../index/index?id={{id}}" plain>
        <image class="news-icon" src="https://hbimg.huabanimg.com/deb427e3df46fa36177cf43e28494ada7db19f72668-iwUuto_fw658" />
      </button>
    </view>
  </view>
  <!-- tab切换 -->
  <view style="position: relative;" class="asdf">
    <scroll-view scroll-left="-100rpx" scroll-left="{{scrollLeft}}" scroll-with-animation class="topScroll left-center" style="width: 670rpx;" bindscrolltoupper="upper" bindscroll="scroll" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}" scroll-x>
      <block wx:for="{{categoryList}}" wx:key>
        <view class="tab-item {{currentTab==index?'tab-active':''}}" data-id="{{item.id}}" data-current="{{index}}" bindtap="clickTap">
          {{item.name}}
        </view>
      </block>
    </scroll-view>
    <view class="more-btn" bindtap="toFenlei">
      <image class="icon-2" src="https://hbimg.huabanimg.com/bf2585481aaa378b23ad49295af47d9bb76ff0ec850-7OmMFu_fw658" />
    </view>
  </view>
  <!-- tab切换 END -->
</view>
<!-- 吸附顶部 END -->
<swiper class="xianshimiaosha" style="{{reaHeight}}" current="{{currentTab}}" duration="300" bindchange="swiperTab">
  <swiper-item>
    <view class="item-box-1">
      <!-- 热卖 -->
      <!-- 论播 -->
      <view class='slider-wrap'>
        <swiper class="slider-box" bindchange="swChange" current autoplay="true" circular="true" interval="5000" duration="500">
          <block wx:for="{{imgUrls}}" wx:key>
            <swiper-item data-current="{{index}}">
              <image src="https://app.zhuoyumall.com:444{{item.ad_code}}" bindtap='toLink' bindload='setContainerHeight' data-link="{{item.ad_link}}" class="slide-image" />
            </swiper-item>
          </block>
        </swiper>
        <view class="dots">{{dots}}/{{imgUrlsLen}}</view>
      </view>
      <!-- 论播 END -->
      <!-- nav -->
      <view class="nav-btn">
        <block wx:for="{{categoryList}}" wx:key>
          <view class="nav-item" data-currentTab="{{index}}" data-id="{{item.id}}" bindtap="{{item.func == 'more' ? 'more' : 'tapchange'}}">
            <image class="nav-icon" src="https://app.zhuoyumall.com:444{{item.icon}}" />
            <view class="nav-text">{{item.name}}</view>
          </view>
        </block>
        <!-- <block wx:for="{{navBtn}}" wx:key>
          <view class="nav-item" data-currentTab="{{index}}" bindtap="{{item.func == 'more' ? 'more' : 'tapchange'}}">
            <image class="nav-icon" src="{{item.url}}" />
            <view class="nav-text">
              {{item.title}}
            </view>
          </view>
        </block> -->
